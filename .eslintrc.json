{
  "globals": {
    "H5P": true,
    "H5PIntegration": true,
    "H5PEditor": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "mocha": true,
    "node": true
  },
  "extends": [
    "eslint:recommended", "plugin:react/recommended", "eslint-config-ndla-h5p"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "valid-jsdoc": "off",
    "no-restricted-globals": [2, "event"],
    "react/prop-types": 0
  }
}
